@article{natura,
	title = {Les informations relatives à la faune du Nord dans le Liber de natura rerum de Thomas de Cantimpré},
	rights = {{RursuSpicae}},
	issn = {2557-8839},
	url = {https://journals.openedition.org/rursuspicae/1445#tocto1n6},
	doi = {10.4000/rursuspicae.1445},
	abstract = {La faune arctique et subarctique, très rarement évoquée dans les textes de l’Antiquité, est peu à peu découverte par les hommes du Moyen Âge, notamment à travers les contacts avec les peuples du Nord et les échanges et activités maritimes et commerciaux. Parfois, ces informations nouvelles permettent aux auteurs latins d’enrichir ou de préciser les données fragmentaires transmises par Aristote, Pline ou Solin. Cette étude s’intéresse à ce type d’information relative à la faune septentrionale dans le Liber de natura rerum ({LDNR}) de Thomas de Cantimpré, à travers les mentions géographiques données par l’auteur, et l’identification zoologique des espèces. Les références de Thomas sur la faune du Nord sont comparées avec celles présentes dans les traités animaliers d’Alexandre Neckam, Vincent de Beauvais, Barthélemy l’Anglais, Albert le Grand, pour évaluer les ressemblances et discordances dans l’approche de la faune nordique.},
	number = {3},
	journaltitle = {{RursuSpicae}. Transmission, réception et réécriture de textes, de l’Antiquité au Moyen Âge},
	author = {Buquet, Thierry},
	urldate = {2022-06-30},
	date = {2020-12-30},
	langid = {french},
	note = {Number: 3
Publisher: Université Nice-Sophia Antipolis {\textbar} {IRHT} - Institut de recherche et d’histoire des textes},
	keywords = {Albert le Grand, animal, encyclopédies médiévales, faune arctique, Thomas de Cantimpré},
	file = {Full Text PDF:/Users/elsafalcoz/Zotero/storage/86YRNBH9/Buquet - 2020 - Les informations relatives à la faune du Nord dans.pdf:application/pdf},
